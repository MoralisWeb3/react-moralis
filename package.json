{
  "name": "react-moralis",
  "version": "0.2.6",
  "description": "Hooks and components to use Moralis in a React app",
  "keywords": [
    "moralis",
    "react",
    "react-hooks",
    "web3",
    "ethereum",
    "dapp",
    "frontend",
    "javascript",
    "typescript",
    "metamask"
  ],
  "homepage": "https://github.com/MoralisWeb3/react-moralis",
  "repository": {
    "type": "git",
    "url": "https://github.com/MoralisWeb3/react-moralis"
  },
  "bugs": {
    "url": "https://github.com/MoralisWeb3/react-moralis/issues"
  },
  "license": "MIT",
  "author": "Erno Wever <erno@ernowever.com>",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "engines": {
    "node": ">=v12.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@commitlint/prompt": "^12.1.1",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "8.1.0",
    "commitizen": "^4.2.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "moralis": "latest",
    "prettier": "^2.2.1",
    "rollup": "^2.45.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-watch": "^4.3.1",
    "standard-version": "^9.2.0",
    "ts-loader": "^8.1.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "moralis": ">=0.0.80",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -cw",
    "prepublishOnly": "yarn run build",
    "lint": "eslint ./src",
    "commit": "npm run git-cz",
    "format": "yarn prettier --write .",
    "format:check": "yarn prettier --check .",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha --no-verify",
    "release:minor": "standard-version --release-as minor --no-verify",
    "release:patch": "standard-version --release-as patch --no-verify",
    "release:major": "standard-version --release-as major --no-verify",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{js,ts,html,css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "immer": "^9.0.1",
    "use-immer": "^0.5.1"
  }
}
